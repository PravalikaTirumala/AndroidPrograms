<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="notificationapp.example.com.notificationapp">
    
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM"/>
    


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivityNotification">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".GridDates"></activity>
        <receiver android:name=".MoreInformationNoti"></receiver>
        <activity android:name=".RepeatingActivity"></activity>
    </application>

</manifest>